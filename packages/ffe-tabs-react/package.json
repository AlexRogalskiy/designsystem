{
  "name": "ffe-tabs-react",
  "version": "1.1.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --compilers js:babel-core/register src/**/*.test.js",
    "test": "npm run test:spec && npm run test:nsp",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "start": "budo examples/tabs.js --open --live --title \"FFE Tabs React\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "example": "babel-node docs/create.js > example.html",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s --json | grep -q \\\"${npm_package_version}\\\""
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "budo": "10.0.4",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.22.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.9.0",
    "eslint-config-ffe": "^2.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-react": "^5.0.1",
    "ffe-core": "^10.1.2",
    "ffe-tabs": "^3.0.3",
    "mocha": "^2.4.5",
    "node-lessify": "^0.1.4",
    "nsp": "^2.2.0",
    "onchange": "2.4.0",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "peerDependencies": {
    "ffe-core": "^10.1.2",
    "ffe-tabs": "^3.0.3",
    "react": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "*"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": "",
  "author": "SpareBank1",
  "license": "",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
