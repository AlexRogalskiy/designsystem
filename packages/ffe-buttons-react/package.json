{
  "name": "ffe-buttons-react",
  "version": "6.2.0",
  "description": "React implementation of ffe-buttons",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js -d lib/. --ignore=*.test.js src/.",
    "lint": "eslint src/.",
    "test": "npm run test:spec && npm run test:nsp",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require src/util/test-setup.js --compilers js:babel-core/register src/**/*.test.js",
    "tdd": "mocha --require src/util/test-setup.js --compilers js:babel-core/register src/**/*.test.js -w",
    "start": "budo examples/buttons.js --open --live --title \"FFE Buttons\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "***REMOVED***"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "babelify": "^8.0.0",
    "budo": "^10.0.3",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.22.0",
    "enzyme": "^2.3.0",
    "eslint": "^4.0.0",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^7.4.0",
    "ffe-buttons": "^5.0.1",
    "ffe-core": "^10.1.1",
    "mocha": "^2.4.5",
    "mocha-tap13": "^1.0.2",
    "node-lessify": "^0.1.5",
    "nsp": "^3.1.0",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "sinon": "^4.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-react-remove-prop-types",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "ffe-icons-react": "^2.4.17",
    "prop-types": "*"
  },
  "license": "UNLICENSED"
}
