{
  "name": "ffe-radio-button-react",
  "version": "1.2.2",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register --reporter mocha-tap13 src/**/*.test.js",
    "test": "npm run test:spec && npm run test:nsp",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "enzyme": "^2.0.0",
    "eslint": "^3.12.2",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "jsdom": "^8.4.1",
    "mocha": "^2.4.5",
    "mocha-tap13": "0.0.3",
    "nsp": "^2.2.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "sinon": "^1.17.3"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "nfe-hash": "^1.1.0",
    "prop-types": "*"
  }
}
