{
    "name": "ffe-checkbox-react",
    "version": "4.4.8",
    "main": "lib/Checkbox.js",
    "scripts": {
        "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
        "watch": "onchange 'src/**.js' -- npm run build",
        "lint": "eslint src/.",
        "start":
            "echo \"No local demo available - use the monorepo's npm start.\"",
        "test:nsp": "nsp check",
        "test:spec":
            "mocha --require babel-register --require ./test-setup.js src/**/*.test.js",
        "test": "npm run test:spec && npm run test:nsp",
        "tdd": "mocha --require babel-register src/**/*.test.js -w",
        "prepublish": "npm run build",
        "example": "babel-node docs/create-docs.js > example.html"
    },
    "peerDependencies": {
        "ffe-form": "^8.1.5"
    },
    "dependencies": {
        "classnames": "^2.2.5",
        "prop-types": "^15.6.0",
        "uuid": "^3.1.0"
    },
    "devDependencies": {
        "budo": "^8.3.0",
        "chai": "^4.1.2",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.13.1",
        "ffe-core": "^10.2.4",
        "ffe-form": "^8.1.5",
        "mocha": "^4.1.0",
        "mocha-tap13": "^1.0.2",
        "node-lessify": "^0.1.5",
        "nsp": "^3.1.0",
        "onchange": "3.2.1",
        "react": "^16.1.1",
        "react-addons-test-utils": "^15.0.1",
        "react-dom": "^16.1.1",
        "sinon": "^4.1.3"
    },
    "files": ["lib", "example.html", "*.js"]
}
