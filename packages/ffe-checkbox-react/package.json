{
    "name": "ffe-checkbox-react",
    "version": "4.4.6",
    "main": "lib/Checkbox.js",
    "scripts": {
        "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
        "watch": "onchange 'src/**.js' -- npm run build",
        "lint": "eslint src/.",
        "start": "budo examples/checkbox.js --open --live --title \"FFE Checkbox React\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
        "test:nsp": "nsp check",
        "test:spec": "mocha --require babel-register --require ./test-setup.js src/**/*.test.js",
        "test": "npm run test:spec && npm run test:nsp",
        "tdd": "mocha --require babel-register src/**/*.test.js -w",
        "prepublish": "npm run build",
        "example": "babel-node docs/create-docs.js > example.html"
    },
    "peerDependencies": {
        "ffe-form": "1.x - 8.x"
    },
    "dependencies": {
        "classnames": "^2.2.5",
        "prop-types": "^15.6.0",
        "sdbm": "^1.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-plugin-react-remove-prop-types": "^2.0.2",
        "babel-plugin-remove-proptypes": "^1.0.3",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.23.0",
        "babelify": "^7.3.0",
        "budo": "^8.3.0",
        "chai": "^4.1.2",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.13.1",
        "ffe-core": "^10.1.1",
        "ffe-form": "^8.1.1",
        "mocha": "^4.1.0",
        "mocha-tap13": "^1.0.2",
        "node-lessify": "^0.1.5",
        "nsp": "^3.1.0",
        "onchange": "3.2.1",
        "react": "^16.1.1",
        "react-addons-test-utils": "^15.0.1",
        "react-dom": "^16.1.1",
        "sinon": "^4.1.3"
    },
    "files": [
        "lib",
        "example.html",
        "*.js"
    ],
    "babel": {
        "presets": [
            "es2015",
            "react"
        ],
        "plugins": [
            "react-remove-prop-types",
            "transform-object-rest-spread"
        ]
    }
}
