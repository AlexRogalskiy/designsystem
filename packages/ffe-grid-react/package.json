{
    "name": "ffe-grid-react",
    "version": "6.0.4",
    "main": "lib/index.js",
    "scripts": {
        "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
        "watch": "onchange 'src/**.js' -- npm run build",
        "lint": "eslint src/.",
        "lint:fix": "eslint --fix src/.",
        "test": "npm run test:spec && npm run test:nsp",
        "test:nsp": "nsp check",
        "test:spec":
            "cross-env NODE_ENV=test mocha -G --require babel-register src/**/*.test.js --require ./src/test/setup.test.js",
        "test:watch":
            "cross-env NODE_ENV=test mocha -G --require babel-register src/**/*.test.js -w --require ./src/test/setup.test.js",
        "tdd": "npm run test:watch",
        "example": "babel-node docs/create.js > example.html",
        "prepublish": "npm run build"
    },
    "dependencies": {
        "classnames": "^2.2.5"
    },
    "peerDependencies": {
        "ffe-grid": "^6.0.4"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-core": "^6.26.0",
        "babel-plugin-syntax-trailing-function-commas": "^6.20.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.23.0",
        "chai": "^4.1.2",
        "cross-env": "^5.1.3",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.13.1",
        "expect.js": "^0.3.1",
        "ffe-core": "^9.2.0",
        "ffe-grid": "^6.0.4",
        "jsdom": "^11.5.1",
        "mocha": "^4.1.0",
        "nsp": "^3.1.0",
        "onchange": "3.2.1",
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "sinon": "^4.1.3"
    },
    "files": ["lib", "*.js"],
    "repository": {
        "type": "git",
        "url": "ssh://git@stash.intern.sparebank1.no:22/ffe/ffe-monorepo.git"
    },
    "author": "SpareBank1",
    "license": "UNLICENSED"
}
