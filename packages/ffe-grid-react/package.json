{
  "name": "ffe-grid-react",
  "version": "6.0.1",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/.",
    "lint:fix": "eslint --fix src/.",
    "test": "npm run test:spec && npm run test:nsp",
    "test:nsp": "nsp check",
    "test:spec": "cross-env NODE_ENV=test mocha -G --require babel-register src/**/*.test.js --require ./src/test/setup.test.js",
    "test:watch": "cross-env NODE_ENV=test mocha -G --require babel-register src/**/*.test.js -w --require ./src/test/setup.test.js",
    "tdd": "npm run test:watch",
    "example": "babel-node docs/create.js > example.html",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "ffe-grid": "^6.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.20.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.5",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^3.13.0",
    "expect.js": "^0.3.1",
    "ffe-core": "^9.2.0",
    "ffe-grid": "^6.0.0",
    "jsdom": "^11.2.0",
    "mocha": "^3.2.0",
    "nsp": "^2.6.2",
    "onchange": "3.2.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "sinon": "^2.3.6"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.intern.sparebank1.no:22/ffe/ffe-monorepo.git"
  },
  "author": "SpareBank1",
  "license": "UNLICENSED"
}
