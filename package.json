{
  "name": "ffe-grid-react",
  "version": "0.0.0",
  "main": "lib/ffe-grid-react.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/.",
    "test": "npm run test:spec && npm run test:nsp",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "test:watch": "mocha --require babel-register src/**/*.test.js -w",
    "tdd": "npm run test:watch",
    "example": "babel-node docs/create.js > example.html",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "devDependencies": {
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-jsx-a11y" : "1.2.2",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "enzyme": "^2.0.0",
    "react-dom": "^15.0.0",
    "onchange": "2.4.0",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "eslint-config-ffe": "^2.0.0",
    "mocha": "^2.4.5",
    "nsp": "^2.2.0"
  },
  "publishConfig": {
    "registry": "https://nexus.intern.sparebank1.no/nexus/content/repositories/npm-internal"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": "",
  "author": "SpareBank1",
  "license": "",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
