{
  "name": "ffe-datepicker-react",
  "version": "2.7.0",
  "main": "lib/ffe-datepicker-react.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "watch": "onchange 'src/**/*.js' -- npm run build",
    "pretest": "eslint src/.",
    "lint": "npm run pretest",
    "test": "mocha --require src/util/test-setup.js --compilers js:babel-core/register 'src/**/*.test.js'",
    "posttest": "nsp check",
    "test:watch": "npm test -- -w",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "start": "budo examples/datepicker.js --open --live --title \"FFE Datepicker\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "start:dateinput": "budo examples/dateinput.js --open --live --title \"FFE Dateinput\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "start:calendar": "budo examples/calendar.js --open --live --title \"FFE Calendar\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "budo": "^10.0.4",
    "chai": "^4.1.2",
    "chai-enzyme": "1.0.0-beta.0",
    "enzyme": "^3.1.0",
    "eslint": "^3.0.0",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "ffe-core": "^10.1.1",
    "ffe-form": "^8.1.1",
    "ffe-icons-react": "^2.4.17",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "node-lessify": "^0.1.5",
    "nsp": "^2.8.1",
    "onchange": "^3.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.14.0"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js",
    "*.less"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "peerDependencies": {
    "ffe-datepicker": "3.x - 4.x",
    "ffe-form": "2.x - 8.x",
    "react": "15.x || 16.x"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "ffe-datepicker": "^4.2.3",
    "prop-types": "15.x",
    "uuid": "^3.1.0"
  }
}
