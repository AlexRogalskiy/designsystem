{
  "name": "ffe-accordion-react",
  "version": "3.1.1",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/.",
    "start": "budo examples/accordion.js --open --live --title \"FFE Accordion React\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "test": "npm run test:spec && npm run test:nsp",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "example": "babel-node docs/create.js > example.html",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "peerDependencies": {
    "ffe-accordion": "^3.0.0",
    "ffe-core": "*",
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "ffe-icons-react": "^2.4.17",
    "prop-types": "^15.6.0",
    "react-collapse": "^4.0.3",
    "react-motion": "^0.5.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.3.0",
    "budo": "8.3.0",
    "chai": "^3.5.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "3.12.2",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "ffe-accordion": "^3.0.0",
    "ffe-core": "^10.1.1",
    "mocha": "^3.2.0",
    "node-lessify": "^0.1.4",
    "nsp": "^2.2.0",
    "onchange": "3.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sinon": "^2.1.0"
  },
  "publishConfig": {
    "registry": "https://nexus.intern.sparebank1.no/nexus/content/repositories/npm-internal"
  },
  "files": [
    "lib",
    "example.html",
    "*.js"
  ],
  "repository": "ssh://git@stash.intern.sparebank1.no:22/ffe/ffe-accordion-react.git",
  "author": "SpareBank 1",
  "license": "ISC"
}
