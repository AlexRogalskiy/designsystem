{
  "name": "ffe-datepicker-react",
  "version": "1.1.5",
  "main": "lib/ffe-datepicker-react.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "watch": "onchange 'src/**/*.js' -- npm run build",
    "pretest": "eslint src/.",
    "test": "mocha --require babel-register 'src/**/*.test.js'",
    "posttest": "nsp check",
    "test:watch": "npm test -- -w",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "start": "budo examples/datepicker.js --open --live --title \"FFE Datepicker\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "start:dateinput": "budo examples/dateinput.js --open --live --title \"FFE Dateinput\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "start:calendar": "budo examples/calendar.js --open --live --title \"FFE Calendar\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.1",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.2",
    "ffe-core": "^6.2.0",
    "ffe-icons-react": "^0.4.1",
    "mocha": "^2.5.3",
    "node-lessify": "^0.1.4",
    "nsp": "^2.2.0",
    "onchange": "^2.3.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.0.1",
    "sinon": "^1.17.4"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js",
    "*.less"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "peerDependencies": {
    "ffe-core": "^6.2.0",
    "react": "^15.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "ffe-datepicker": "^1.0.3"
  }
}
