{
  "name": "ffe-datepicker-react",
  "version": "2.4.0",
  "main": "lib/ffe-datepicker-react.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "watch": "onchange 'src/**/*.js' -- npm run build",
    "pretest": "eslint src/.",
    "lint": "npm run pretest",
    "test": "mocha --require src/util/test-setup.js --compilers js:babel-core/register 'src/**/*.test.js'",
    "posttest": "nsp check",
    "test:watch": "npm test -- -w",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "start": "budo examples/datepicker.js --open --live --title \"FFE Datepicker\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "start:dateinput": "budo examples/dateinput.js --open --live --title \"FFE Dateinput\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "start:calendar": "budo examples/calendar.js --open --live --title \"FFE Calendar\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "enzyme": "^2.3.0",
    "eslint": "^3.12.2",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "ffe-core": "^9.2.0",
    "ffe-form": "^5.1.0",
    "ffe-icons-react": "^2.4.3",
    "jsdom": "^10.0.0",
    "mocha": "^2.5.3",
    "node-lessify": "^0.1.4",
    "nsp": "^2.2.0",
    "onchange": "^2.3.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.0.1",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.9.0"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js",
    "*.less"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "peerDependencies": {
    "ffe-datepicker": "3.x - 4.x",
    "ffe-form": "2.x - 6.x",
    "react": "15.x"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "ffe-datepicker": "^4.0.0"
  }
}
