{
  "name": "ffe-expandable-react",
  "version": "0.0.0",
  "main": "lib/ffe-expandable-react.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --compilers js:babel-core/register --require ./test.config.js  src/**/*.test.jsx",
    "test": "npm run test:spec && npm run test:nsp",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "enzyme": "^2.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.2",
    "install": "^0.7.3",
    "mocha": "^2.4.5",
    "nsp": "^2.2.0",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "eslintConfig": {
    "extends": "airbnb"
  },
  "repository": "",
  "author": "",
  "license": "",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
