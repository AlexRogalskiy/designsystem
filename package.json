{
  "name": "ffe-message-box-react",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/.",
    "start": "budo examples/message-box.js --open --live --title \"FFE Message Box React\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "test": "npm run test:spec && npm run test:nsp",
    "example": "babel-node docs/create-docs.js > example.html",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "peerDependencies": {
    "ffe-message-box": "^3.0.0",
    "ffe-core": "^8.1.0",
    "ffe-icons-react": "^1.1.0"
  },
  "devDependencies": {
    "ffe-message-box": "^3.0.0",
    "ffe-core": "^8.1.0",
    "ffe-icons-react": "^1.1.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "budo": "^9.1.0",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "mocha": "^3.0.2",
    "mocha-tap13": "1.0.4",
    "node-lessify": "^0.1.4",
    "nsp": "^2.6.1",
    "onchange": "3.0.2",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "15.3.1"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "example.html",
    "*.js"
  ],
  "repository": "",
  "author": "SpareBank1",
  "license": ""
}
