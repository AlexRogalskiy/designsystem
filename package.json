{
  "name": "ffe-expandable-react",
  "version": "1.0.7",
  "main": "lib/ffe-expandable-react.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "lint": "eslint src/.",
    "start": "budo examples/examples.js --open --live --title \"FFE Expandable React\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "test:nsp": "nsp check",
    "test:spec": "mocha --compilers js:babel-core/register --require ./test.config.js  src/**/*.test.jsx",
    "test": "npm run test:spec && npm run test:nsp",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.3.0",
    "budo": "^9.2.2",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "enzyme": "^2.0.0",
    "eslint": "^3.12.2",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "ffe-core": "^8.1.2",
    "install": "^0.7.3",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "node-lessify": "^0.1.4",
    "nsp": "^2.2.0",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "eslintConfig": {
    "extends": "airbnb"
  },
  "repository": "",
  "author": "",
  "license": "",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "dependencies": {
    "react-height": "^2.1.1"
  }
}
