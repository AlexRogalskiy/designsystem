{
  "name": "ffe-message-box-react",
  "version": "0.2.2",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "test": "npm run test:spec && npm run test:nsp",
    "example": "babel-node docs/create-docs.js > example.html",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "peerDependencies": {
    "ffe-message-box": "^2.0.2",
    "ffe-core": "*",
    "ffe-icons-react": "^0.4.1"
  },
  "devDependencies": {
    "ffe-message-box": "^2.0.2",
    "ffe-core": "*",
    "ffe-icons-react": "^0.4.1",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.12.0",
    "mocha": "^2.5.3",
    "mocha-tap13": "1.0.4",
    "nsp": "^2.4.0",
    "onchange": "2.5.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "15.1.0"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "example.html",
    "*.js"
  ],
  "repository": "",
  "author": "SpareBank1",
  "license": ""
}
