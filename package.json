{
  "name": "ffe-icons-react",
  "version": "0.5.3",
  "description": "React komponent for ffe-icons",
  "main": "ffe-icons-react.js",
  "scripts": {
    "build": "npm run lint && npm run ffe:icons && npm run build:iconlist && npm run build:jsx-components && npm run lint:jsx && npm run build:babel",
    "build:babel": "babel jsx -d .",
    "build:iconlist": "babel-node ./src/build-iconlist.js",
    "build:jsx-components": "babel-node ./src/build-jsx-components.js",
    "lint": "eslint src",
    "lint:jsx": "eslint ./jsx/*.jsx",
    "test:nsp": "nsp check",
    "clean": "babel-node ./src/clean.js",
    "ffe:icons": "gulp --no-color --gulpfile node_modules/ffe-icons/gulpfile.js --opts=../../svg.sprite.config.json",
    "server": "npm run build && npm run build:iconlist && webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "***REMOVED***"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "license": "",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.9.0",
    "eslint-config-ffe": "^2.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "^5.0.1",
    "ffe-icons": "4.5.0",
    "gulp": "^3.9.1",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "nsp": "^2.2.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "*.js"
  ]
}
