{
  "name": "ffe-message-box-react",
  "version": "3.1.5",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "npm run pretest",
    "start": "budo examples/message-box.js --open --live --title \"FFE Message Box React\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "pretest": "eslint src/.",
    "posttest": "nsp check",
    "test:watch": "npm test -- --watch",
    "test": "mocha --require babel-register src/**/*.test.js",
    "example": "babel-node docs/create-docs.js > example.html",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "peerDependencies": {
    "ffe-message-box": "^3.0.0",
    "ffe-core": "^8.1.0",
    "ffe-icons-react": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "budo": "^8.3.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.12.2",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "ffe-core": "^8.1.0",
    "ffe-icons-react": "^2.3.0",
    "ffe-message-box": "^3.0.0",
    "mocha": "^2.5.3",
    "mocha-tap13": "1.0.4",
    "node-lessify": "^0.1.4",
    "nsp": "^2.4.0",
    "onchange": "2.5.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "example.html",
    "*.js"
  ],
  "repository": "",
  "author": "SpareBank1",
  "license": ""
}
