{
  "name": "ffe-file-upload-react",
  "version": "1.2.0",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run server",
    "server": "./node_modules/.bin/webpack-dev-server --progress --color",
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint --ext .js,.jsx src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --compilers babel-register src/**/*.test.js",
    "test:watch": "mocha --compilers babel-register src/**/*.test.js -w  --watch-extensions jsx",
    "test": "npm run test:spec && npm run test:nsp",
    "tdd": "npm run test:watch",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s --json | grep -q \\\"${npm_package_version}\\\""
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^4.1.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-ffe": "^6.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "ffe-core": "^10.0.0",
    "ffe-form": "^7.0.0",
    "file-loader": "^0.11.1",
    "less": "^2.6.0",
    "less-loader": "^4.0.5",
    "mocha": "^3.4.2",
    "nsp": "^2.2.0",
    "onchange": "3.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.8",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "peerDependencies": {
    "react": "^15.x",
    "react-dom": "15.x",
    "prop-types": "*"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": "",
  "author": "SpareBank1",
  "license": "",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
