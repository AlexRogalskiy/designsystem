{
  "name": "ffe-buttons-react",
  "version": "5.1.2",
  "description": "React implementation of ffe-buttons",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js -d lib/. --ignore=*.test.js src/.",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "test": "npm run test:spec && npm run test:nsp",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "***REMOVED***"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "enzyme": "^2.1.0",
    "eslint": "^3.12.2",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "mocha": "^2.4.5",
    "mocha-tap13": "^1.0.2",
    "nsp": "^2.2.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "sinon": "^1.17.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-react-remove-prop-types",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "ffe-icons-react": "^2.1.0"
  }
}
