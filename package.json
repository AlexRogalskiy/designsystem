{
  "name": "ffe-checkbox-react",
  "version": "1.0.0",
  "main": "lib/checkbox.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "test:readme": "readme-assert",
    "test": "npm run test:spec && npm run test:readme && npm run test:nsp",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.2",
    "mocha": "^2.4.5",
    "mocha-tap13": "0.0.3",
    "nsp": "^2.2.0",
    "readme-assert": "^2.0.1"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
