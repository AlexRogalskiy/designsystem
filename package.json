{
  "name": "ffe-dropdown-react",
  "version": "1.4.2",
  "main": "lib/dropdown.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "test": "npm run test:spec && npm run test:nsp",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "prepublish": "npm run build",
    "example": "babel-node docs/create-docs.js > example.html",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "peerDependencies": {
    "ffe-core": "^6.0.0"
  },
  "dependencies": {
    "nfe-hash": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-plugin-react-remove-prop-types": "^2.0.2",
    "babel-plugin-remove-proptypes": "^1.0.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "enzyme": "2.2.0",
    "eslint": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^4.2.2",
    "mocha": "^2.4.5",
    "nsp": "^2.2.0",
    "react": "*",
    "react-dom": "*",
    "react-addons-test-utils": "^15.0.1",
    "sinon": "^1.17.3"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "example.html",
    "*.js"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-remove-prop-types"
    ]
  }
}
