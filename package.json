{
  "name": "ffe-chart-donut-react",
  "version": "1.0.0",
  "description": "Donut chart in SVG",
  "scripts": {
    "build": "babel -d lib/ src/ && cp src/ffe-chart-donut.less lib/",
    "build:example": "npm run build:less && babel-node example/example.js",
    "build:less": "lessc example/example.less example/example.css",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint --fix src/ test/",
    "start": "hjs-dev-server",
    "test": "npm run test:spec && npm run test:nsp && npm run build:example && ./run_visual-tests.sh",
    "test:nsp": "nsp check",
    "test:spec": "mocha -G --compilers js:babel-core/register --require ./test/setup.test.js test/**/*.test.js",
    "test:watch": "npm run tdd",
    "tdd": "mocha -G --compilers js:babel-core/register --require ./test/setup.test.js test/**/*.test.js -w",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s --json | grep -q \\\"${npm_package_version}\\\""
  },
  "keywords": [
    "ffe"
  ],
  "peerDependencies": {
    "react": "^15.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-ffe": "^6.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "expect.js": "^0.3.1",
    "ffe-core": "^9.2.0",
    "ffe-visual-tests-support": "^2.1.2",
    "hjs-webpack": "^9.1.0",
    "jsdom": "^9.12.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "mocha": "^3.2.0",
    "nsp": "^2.6.3",
    "onchange": "3.2.1",
    "postcss-loader": "^1.3.3",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "style-loader": "^0.16.1"
  },
  "files": [
    "lib",
    "*.js",
    "*.less"
  ],
  "main": "lib/index.js",
  "author": "SpareBank 1",
  "repository": {
    "type": "git",
    "url": "***REMOVED***"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "license": "ISC"
}
