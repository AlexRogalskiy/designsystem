{
  "name": "ffe-form",
  "version": "1.0.0",
  "description": "Form-/skjemapakke for Felles Front End",
  "main": "less/form.less",
  "scripts": {
    "compile": "npm run lint && lessc examples/example.less examples/example.css && cp -R node_modules/ffe-core/fonts/ examples/fonts/ && echo \"$(date +%T): Updated example\"",
    "lint": "lessc --lint examples/example.less",
    "start": "opn examples/example.html & watch \"npm run -s compile\" less/",
    "test:nsp": "nsp check",
    "test": "npm run lint && npm run test:nsp",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "preversion": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.intern.sparebank1.no:22/ffe/ffe-form.git"
  },
  "homepage": "https://stash.intern.sparebank1.no/projects/FFE/repos/ffe-form/browse",
  "documentation": [
    {
      "type": "html",
      "file": "examples/example.html",
      "select": "#ffedoc"
    },
    {
      "type": "markdown",
      "file": "README.md"
    }
  ],
  "devDependencies": {
    "ffe-core": "^8.0.2",
    "ffe-visual-tests-support": "1.0.1",
    "gemini": "4.3.0",
    "less": "^2.6.1",
    "nsp": "^2.2.0",
    "opn-cli": "^3.1.0",
    "watch": "^0.19.2"
  },
  "peerDependencies": {
    "ffe-core": "^8.0.2"
  },
  "publishConfig": {
    "registry": "https://nexus.intern.sparebank1.no/nexus/content/repositories/npm-internal"
  },
  "files": [
    "less"
  ],
  "keywords": [
    "frontend",
    "front",
    "end",
    "style",
    "guide",
    "sb1",
    "styling",
    "design",
    "form",
    "checkbox",
    "dropdown",
    "error summary",
    "fieldset",
    "form label",
    "info message",
    "input",
    "input field",
    "input group",
    "radio block",
    "radio button",
    "radio input",
    "radio switch"
  ],
  "author": "SpareBank1",
  "license": "ISC"
}
