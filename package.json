{
  "name": "ffe-form",
  "version": "4.1.4",
  "description": "Form-/skjemapakke for Felles Front End",
  "main": "less/form.less",
  "scripts": {
    "compile": "npm run lint && lessc examples/examples.less examples/examples.css --autoprefix && cp -R node_modules/ffe-core/fonts/ examples/fonts/ && echo \"$(date +%T): Updated example\"",
    "lint": "lessc --lint examples/examples.less && stylelint less/*.less",
    "start": "watch \"npm run -s compile\" less/",
    "test:nsp": "nsp check",
    "test:visual": "./run_visual-tests.sh",
    "test": "npm run lint && npm run test:nsp && npm run test:visual",
    "examples": "lessc examples/examples.less examples/examples.css --autoprefix && mkdirp examples/fonts && cp -R node_modules/ffe-core/fonts/* examples/fonts/ && echo Now open /examples/[component].html to see styleguide.",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "preversion": "npm run lint",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "***REMOVED***"
  },
  "homepage": "***REMOVED***",
