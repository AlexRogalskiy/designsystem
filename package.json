{
  "name": "ffe-grid-react",
  "version": "4.0.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/.",
    "lint:fix": "eslint --fix src/.",
    "test": "npm run test:spec && npm run test:nsp",
    "test:nsp": "nsp check",
    "test:spec": "mocha -G --require babel-register src/**/*.test.js --require ./src/test/setup.test.js",
    "test:watch": "mocha -G --require babel-register src/**/*.test.js -w --require ./src/test/setup.test.js",
    "tdd": "npm run test:watch",
    "example": "babel-node docs/create.js > example.html",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "ffe-grid": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.20.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.0",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "expect.js": "^0.3.1",
    "ffe-core": "^9.2.0",
    "ffe-grid": "^5.0.0",
    "mocha": "^3.2.0",
    "nsp": "^2.6.2",
    "onchange": "3.2.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "publishConfig": {
    "registry": "https://nexus.intern.sparebank1.no/nexus/content/repositories/npm-internal"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://bitbucket.intern.sparebank1.no/scm/ffe/ffe-grid-react.git"
  },
  "author": "SpareBank1",
  "license": "UNLICENSED"
}
