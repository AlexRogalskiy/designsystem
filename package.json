{
  "name": "ffe-dropdown-react",
  "version": "2.5.2",
  "main": "lib/dropdown.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "lint": "eslint src/.",
    "start": "budo examples/dropdown.js --open --live --title \"FFE Dropdown React\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "test": "npm run test:spec && npm run test:nsp",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "prepublish": "npm run build",
    "example": "babel-node docs/create-docs.js > example.html",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "peerDependencies": {
    "ffe-core": "^8.0.2",
    "ffe-form": "1.x || 2.x || 3.x || 4.x",
    "ffe-spinner": "^1.0.0"
  },
  "dependencies": {
    "ffe-core": "^8.0.2",
    "nfe-hash": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-plugin-react-remove-prop-types": "^2.0.2",
    "babel-plugin-remove-proptypes": "^1.0.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.3.0",
    "budo": "^8.3.0",
    "chai": "^3.5.0",
    "enzyme": "2.2.0",
    "eslint": "^3.12.2",
    "eslint-config-ffe": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "mocha": "^2.4.5",
    "node-lessify": "^0.1.4",
    "nsp": "^2.2.0",
    "react": "*",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "*",
    "sinon": "^1.17.3"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "example.html",
    "*.js"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-remove-prop-types"
    ]
  }
}
