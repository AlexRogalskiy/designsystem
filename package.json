{
  "name": "ffe-button-react",
  "version": "2.4.1",
  "description": "React implementation of ffe-button",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "test": "npm run test:spec && npm run test:nsp",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "publishConfig": {
    "registry": "https://nexus.intern.sparebank1.no/nexus/content/repositories/npm-internal"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.intern.sparebank1.no:22/ffe/ffe-button-react.git"
  },
  "peerDependencies": {
    "ffe-core": "6.x"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "enzyme": "^2.1.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "mocha": "^2.4.5",
    "mocha-tap13": "^1.0.2",
    "nsp": "^2.2.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "sinon": "^1.17.3"
  },
  "babel": {
