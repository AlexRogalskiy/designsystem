{
  "name": "ffe-radio-button-react",
  "version": "0.1.0",
  "main": "lib/radio-button.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register --reporter mocha-tap13 src/**/*.test.js",
    "test:readme": "readme-assert",
    "test": "npm run test:spec && npm run test:readme && npm run test:nsp",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}"
  },
  "devDependencies": {
    "eslint-plugin-react": "^4.2.2",
    "react": "^0.14.8",
    "enzyme": "^2.0.0",
    "react-dom": "^0.14.7",
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "mocha": "^2.4.5",
    "mocha-tap13": "0.0.3",
    "nsp": "^2.2.0"
  },
  "publishConfig": {
    "registry": "https://nexus.intern.sparebank1.no/nexus/content/repositories/npm-internal"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "eslintConfig": {
    "extends": "airbnb"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
