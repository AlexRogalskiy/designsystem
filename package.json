{
  "name": "ffe-datepicker-react",
  "version": "0.0.0",
  "main": "lib/ffe-datepicker-react.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/.",
    "build:watch": "onchange 'src/**/*.js' -- npm run build",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "NODE_ENV=test && tape -r babel-register 'src/**/test/*.js' | tap-dot",
    "test:spec:watch": "onchange 'src/**/*.js' -- npm run test:spec",
    "test:readme": "readme-assert",
    "test": "npm run test:spec && npm run test:readme && npm run test:nsp",
    "prepublish": "npm run build",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "start": "budo example/example.js --open --live --title \"FFE Datepicker\" -- -t [ babelify --presets [ es2015 react ] ] -t node-lessify"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.2",
    "node-lessify": "^0.1.4",
    "nsp": "^2.2.0",
    "onchange": "^2.3.0",
    "react-dom": "^15.0.1",
    "readme-assert": "^2.0.1",
    "tap-dot": "^1.0.5",
    "tape": "^4.5.1"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "eslintConfig": {
    "extends": "airbnb"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "peerDependencies": {
    "ffe-core": "^5.3.0"
  },
  "dependencies": {
    "lodash.isdate": "^3.0.3",
    "react": "^15.0.1"
  }
}
