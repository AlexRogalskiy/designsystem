{
  "name": "ffe-checkbox-react",
  "version": "3.0.0",
  "main": "lib/checkbox.js",
  "scripts": {
    "build": "babel -d lib/. --ignore=*.test.js src/. && npm run example",
    "watch": "onchange 'src/**.js' -- npm run build",
    "lint": "eslint src/.",
    "test:nsp": "nsp check",
    "test:spec": "mocha --require babel-register src/**/*.test.js",
    "test": "npm run test:spec && npm run test:nsp",
    "tdd": "mocha --require babel-register src/**/*.test.js -w",
    "prepublish": "npm run build",
    "example": "babel-node docs/create-docs.js > example.html",
    "has-published": "npm show . versions -s | grep -q ${npm_package_version}",
    "postpublish": "git tag ${npm_package_version} && git push --tags"
  },
  "peerDependencies": {
    "ffe-core": "^8.1.0",
    "ffe-form": "1.x || 2.x || 3.x"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "nfe-hash": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-plugin-react-remove-prop-types": "^2.0.2",
    "babel-plugin-remove-proptypes": "^1.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "enzyme": "2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-ffe": "2.0.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "5.0.1",
    "ffe-core": "^8.1.0",
    "ffe-form": "^1.0.1",
    "mocha": "^2.4.5",
    "mocha-tap13": "0.0.3",
    "nsp": "^2.2.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "sinon": "^1.17.3"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "files": [
    "lib",
    "example.html",
    "*.js"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-remove-prop-types",
      "transform-object-rest-spread"
    ]
  }
}
